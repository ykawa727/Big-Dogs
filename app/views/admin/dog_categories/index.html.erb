<div class="container">
  <div class="row">
    <div class="col-sm-10">
      <h4>ワンちゃん犬種一覧・追加</h4>
        <table class>
          <thead>
            <tr>
              <%= form_with model:@dog_category, url:admin_dog_categories_path,local:true do |f| %>
                <td>ジャンル名</td>
                <td><%= f.text_field :category %></td>
                <td><%= f.submit '新規登録', class: "btn btn-success" %></td>
              <% end %>
            </tr>
          </thead>

          <tbody>
            <tr>
              <td>犬種</td>
            </tr>

            <% @dog_categories.each do |dog_category| %>
            <tr>
              <td><%= dog_category.category %></td>
              <td><%= link_to '編集する', edit_admin_dog_category_path(dog_category.id), class: "btn btn-success" %></td>
            </tr>
            <% end %>
          </tbody>
        </table>
    </div>
  </div>
</div>


